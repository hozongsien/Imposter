{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/forbid-prop-types": [
      0,
      { "forbid": [], "checkContextTypes": 0, "checkChildContextTypes": 0 }
    ]
  }
}
